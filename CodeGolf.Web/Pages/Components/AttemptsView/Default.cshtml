@model IReadOnlyList<CodeGolf.Domain.Attempt>;

<span class="title">Attempts</span>

<table class="table is-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Score</th>
            <th>Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var attempt in Model)
        {
            <tr>
                <td>@attempt.UserId</td>
                <td>@attempt.Score</td>
                <td>@attempt.TimeStamp</td>
            </tr>
        }
    </tbody>
</table>